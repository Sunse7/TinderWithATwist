{
  "rules": {
    "prettier/prettier": [
      1,
      {
        "printWidth": 120,
        "trailingComma": "es5",
        "singleQuote": true,
        "bracketSpacing": true,
        "semi": true,
        "jsxBracketSameLine": true,
        "useTabs": false,
        "arrowParens": "avoid",
        "endOfLine":"auto"
      }
    ],
    "no-unused-vars": "off",
    "curly": "off",
    "react-hooks/exhaustive-deps": ["warn"],
    "react/jsx-no-useless-fragment": ["warn"]
  }}